import csv

with open('csv_file.csv', 'r', encoding='utf-8') as f:
    reader = csv.reader(f)
    data = list(reader)

data[0].append('Зарплата')

data[1].append('120000')
data[2].append('100000')
data[3].append('90000')

with open('employees_with_salary.csv', 'w', encoding='utf-8', newline='') as f:
    writer = csv.writer(f)
    writer.writerows(data)
